<script>


d3.json("./companies.json", function (data) {

    var convas = d3.select("body").append("svg")
    .attr("width", 500)
    .attr("height", 500)

    convas.selectAll("rect")
    .data(data)
    .enter()
      .append("rect")
      .attr("width", function(d){ return d.companies; })
      .attr("height", 50)
      .attr("y", function (d) { return d.county; })
      .attr("fill", "blue")


  })


</script>

